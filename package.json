{
  "name": "dnd-inventory",
  "version": "0.1.0",
  "main": "firebaseFunctions.js",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --fix .",
    "prettier:fix": "prettier --write --ignore-unknown .",
    "generate:svgs": "svgr --out-dir src/lib/category/icons --filename-case kebab --typescript --no-dimensions --prettier-config .prettier.js -- assets/icons",
    "generate:borders": "svgr --out-dir src/lib/category/borders --filename-case kebab --typescript --no-dimensions --prettier-config .prettier.js -- assets/borders"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/server": "^11.10.0",
    "@mantine/core": "^5.10.4",
    "@mantine/form": "^5.10.4",
    "@mantine/hooks": "^5.10.4",
    "@mantine/modals": "^5.10.4",
    "@mantine/next": "^5.10.4",
    "@mantine/nprogress": "^5.10.4",
    "@samuel-lewis/components": "^0.4.0",
    "@samuel-lewis/utils": "^0.4.0",
    "@tabler/icons": "^1.119.0",
    "firebase": "^9.16.0",
    "fuse.js": "^6.6.2",
    "next": "13.1.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-firebase-hooks": "^5.1.1",
    "typescript": "4.9.4",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@svgr/cli": "^6.5.1",
    "@types/node": "18.11.18",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "eslint": "8.32.0",
    "eslint-config-next": "13.1.3",
    "eslint-config-samuel-lewis": "^0.4.0",
    "firebase-tools": "^11.21.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "pnpm lint",
    "**/*": "pnpm prettier:fix"
  }
}
